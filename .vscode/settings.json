{
  "eslint.workingDirectories": ["."],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.hover.delay": 500,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "files.exclude": {
    "**/.angular/cache": true
  },
  "search.exclude": {
    "**/.angular/cache": true
  }
}
